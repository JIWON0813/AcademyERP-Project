<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.demo.database.Repository.ConsultMapper">

     <insert id="insertConsult" parameterType="java.util.HashMap">
        INSERT INTO consut VALUES (null,#{name},#{hp},#{schedule},#{memo},sysdate,#{route},#{writer})
    </insert>

    <select id="selectConsult" resultType="ConsultEntity">
        select * from consult
    </select>

</mapper> 